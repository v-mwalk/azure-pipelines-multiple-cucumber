  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Cucumber Task",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/tasks/PublishMultipleCucumberReport/index.js",
      "outputCapture": "std",
      "env": {
        "INPUT_JSONDIR": "${workspaceFolder}/tasks/PublishMultipleCucumberReport/inputs/jv",
        "INPUT_OUTPUTPATH": "${workspaceFolder}/tasks/PublishMultipleCucumberReport/outputs",
        "INPUT_METADATA": "",
        "INPUT_THEME": "bootstrap",
        "INPUT_REPORTSUITEASSCENARIOS": true,
        "SYSTEM_DEFAULTWORKINGDIRECTORY": ""
      },
      "cwd": "${workspaceFolder}/tasks/PublishMultipleCucumberReport"
    }
  ]
}
